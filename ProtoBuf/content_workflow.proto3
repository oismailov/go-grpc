syntax = "proto3";

import "google/protobuf/empty.proto";

service ContentWorkflow {
  rpc New(ContentRequest) returns (ContentResponse) {}
  rpc Get(ContentRequest) returns (ContentResponse) {}
  rpc List(google.protobuf.Empty) returns (stream ContentResponse) {}
}

enum ContentType {
  MARKETING_LANDING_PAGE = 0;
}

message ContentRequest {
  string id = 1;
  string name = 2;
  ContentType type = 3;
}

message ContentResponse {
  string message = 1;
}